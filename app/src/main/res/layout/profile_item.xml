<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <View
        android:id="@+id/profile_divider_hori"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#D8D8D8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/profile_table_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="36dp"
        android:text="@string/tablerow1_title"
        android:textSize="16sp"
        android:textColor="@color/black"
        android:fontFamily="@font/pretendard_medium"
        app:layout_constraintEnd_toStartOf="@+id/profile_divider_vert"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profile_divider_hori" />

    <View
        android:id="@+id/profile_divider_vert"
        android:layout_width="1dp"
        android:layout_height="33dp"
        android:background="#D8D8D8"
        android:orientation="vertical"
        app:layout_constraintTop_toBottomOf="@+id/profile_divider_hori"
        tools:layout_editor_absoluteX="89dp" />

    <TextView
        android:id="@+id/profile_table_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="@string/tablerow1_value"
        android:textSize="16sp"
        android:fontFamily="@font/pretendard_light"
        android:textColor="@color/black"
        app:layout_constraintStart_toEndOf="@+id/profile_divider_vert"
        app:layout_constraintTop_toTopOf="@+id/profile_table_title" />


</androidx.constraintlayout.widget.ConstraintLayout>